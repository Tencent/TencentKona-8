            __ sb(T9, AT, disp);
          }
        } else {
          if (value == 0) {
            __ move(T9, disp);
            __ daddu(AT, AT, T9);
            __ sb(R0, AT, 0);
          } else {
            __ move(T9, disp);
            __ daddu(AT, AT, T9);
            __ move(T9, value);
            __ sb(T9, AT, 0);
          }
        }
      } else {

        if (scale == 0) {
          if( Assembler::is_simm(disp, 8) ) {
            if (value == 0) {
