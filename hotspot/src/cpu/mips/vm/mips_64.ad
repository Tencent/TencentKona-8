instruct convI2B(mRegI dst, mRegI src) %{
  match(Set dst (Conv2B src));

  ins_cost(100);
  format %{ "convI2B    $dst, $src @ convI2B"  %}
  ins_encode %{
    Register dst = as_Register($dst$$reg);
    Register src = as_Register($src$$reg);

    if (dst != src) {
      __ daddiu(dst, R0, 1);
      __ movz(dst, R0, src);
    } else {
      __ move(AT, src);
      __ daddiu(dst, R0, 1);
      __ movz(dst, R0, AT);
    }
  %}

  ins_pipe( ialu_regL_regL );
%}

