    __ c_un_s(fval, fval);    //NaN?
    __ movt(dreg, R0);

    __ bne(AT, dreg, L);
    __ delayed()->lui(T9, 0x8000);

    __ mfc1(AT, fval);
    __ andr(AT, AT, T9);

    __ dsll32(T9, T9, 0);
    __ movn(dreg, T9, AT);

    __ bind(L);
  %}

